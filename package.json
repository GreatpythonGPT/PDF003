{
  "name": "render-pdf-tool",
  "version": "1.0.0",
  "description": "Rendering image PDF generator",
  "main": "dist/electron/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "build": "tsc -p . && vite build",
    "dev": "concurrently -k \"tsc -w -p .\" \"vite\" \"wait-on dist/index.html && electron .\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/material": "^5.15.8",
    "browser-image-compression": "^2.0.0",
    "electron": "^36.4.0",
    "framer-motion": "^10.16.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.7.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "concurrently": "^8.2.0",
    "typescript": "^5.4.2",
    "vite": "^5.0.0",
    "wait-on": "^7.0.1"
  }
}
